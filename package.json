    "prepare": "husky",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "release": "standard-version",
    "compile": "rimraf dist && tsc && tsc --build tsconfig.json"
