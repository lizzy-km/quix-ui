{
  "name": "quix-ui",
  "version": "1.3.16",
  "description": "This is a react component library.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "rollup": " rimraf dist && rollup -c ",
    "test": "jest --watchAll --verbose",
