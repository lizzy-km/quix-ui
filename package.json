  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizzy-km/quix-ui.git"
  },
  "author": "Kaung Myat Soe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lizzy-km/quix-ui/issues"
  },
  "homepage": "https://github.com/lizzy-km/quix-ui#readme",
  "jest": {
    "testEnvironment": "jsdom"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
